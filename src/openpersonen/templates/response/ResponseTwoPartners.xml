                     <!--Optional:-->
                     <ns:voornamen>Media Partner</ns:voornamen>
                     <!--Optional:-->
                     <ns:voorvoegselGeslachtsnaam>de</ns:voorvoegselGeslachtsnaam>
                     <!--Optional:-->
                     <ns:geboortedatum>19690507</ns:geboortedatum>
                     <!--Optional:-->
                     <ns:inp.geboorteplaats>Amsterdam</ns:inp.geboorteplaats>
